{
  "name": "webpack_boilerplate",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --watchAll=false",
    "dev": "webpack serve --env api=local --config ./config/webpack.dev.js",
    "prod": "rm -rf ./dist/* && webpack --env api=dev --config ./config/webpack.prod.js",
    "cy": "cypress run",
    "cy:dev": "cypress open"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/R3tter/webpack_boilerplate.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/R3tter/webpack_boilerplate/issues"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/config/setupTests.js"
    ],
    "transform": {
      "^.+\\.(jsx|js)$": "babel-jest",
      "^.+\\.svg$": "<rootDir>/jest-svg-transformer.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/fileTransformer.js"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      ".+\\.(scss|css)$": "identity-obj-proxy"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/cypress/"
    ]
  },
  "homepage": "https://gitlab.com/R3tter/webpack_boilerplate#readme",
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@svgr/webpack": "^6.2.1",
    "@testing-library/react": "^13.3.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^28.1.2",
    "babel-loader": "^8.2.5",
    "babel-plugin-module-resolver": "^4.1.0",
    "css-loader": "^6.7.1",
    "cypress": "^10.3.0",
    "eslint": "^8.19.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^3.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.2",
    "mini-css-extract-plugin": "^2.6.1",
    "precss": "^4.0.0",
    "prettier": "^2.7.1",
    "prettier-eslint-cli": "^6.0.1",
    "style-loader": "^3.3.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  },
  "dependencies": {
    "@jmind.systems/react-modal": "0.0.27",
    "@jmind.systems/react-status-controller": "0.0.14",
    "aphrodite": "^2.4.0",
    "api-req-redux": "2.0.10",
    "core-js": "^3.23.3",
    "i18next": "^21.8.13",
    "normalize-scss": "^7.0.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.18.0",
    "react-notify-library": "0.0.14",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "redux": "^4.2.0",
    "redux-actions": "^2.6.5",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.4.1",
    "reselect": "^4.1.6",
    "use-form-validation-hook": "^1.0.20"
  }
}
